Host github.com
    Hostname github.com
    ForwardAgent no
    ForwardX11 no
    ForwardX11Trusted no
    User git
    IdentityFile ~/.ssh/dlocksJP_rsa

Host source-americas.it.here.com
    ForwardAgent no
    ForwardX11 no
    PubKeyAuthentication yes
    User snc
    IdentityFile ~/.ssh/locks_snc

Host gerrit.it.here.com
    ForwardAgent no
    ForwardX11 no

Host *.hq.navteq.com
    IdentityFile ~/.ssh/linux_id_rsa

Host *
    ForwardAgent yes
    ForwardX11 yes
    PubKeyAuthentication yes
    StrictHostKeyChecking no
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    GSSAPIKeyExchange yes

